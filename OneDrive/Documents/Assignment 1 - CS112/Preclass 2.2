###Preclass work 2.1
RMSE1 = function(i, j){
  sqrt(mean((i-j)^2))
}
Heyou = read.csv("Heyou data.csv")
Heyoumodel <- lm(avg_ppl_dated_per_yr ~ appearance + gender +1, data=Heyou)
RMSE1(predict(Heyoumodel,Heyou), Heyou$avg_ppl_dated_per_yr)
summary(Heyoumodel)


